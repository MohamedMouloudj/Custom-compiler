VAR_GLOBAL {
   INTEGER B,C;
   FLOAT A;
   CHAR Z[5],R[1],D[10];
}
DECLARATION {
    CONST INTEGER Y = 5;
    CONST INTEGER X = 4;
}

INSTRUCTION {

        Z[2]='k';
        B =(-2.6)-2*(5+6)/3+4;
        X=5;

         IF(A > 5 && B<2){
               B=9;
         }

        A=3; B=1; C=3;
        IF(A == 9 && B == 1){
            A=1;
        }
}