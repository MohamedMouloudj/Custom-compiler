VAR_GLOBAL {
   INTEGER A,B,C;
   CHAR Arr[5];
   FLOAT E[10];
   INTEGER I;
}
DECLARATION {
    CONST INTEGER Y = 5;
    CONST INTEGER X = 4;
}

INSTRUCTION {
        E[0]=1.0;
        Arr[0]='h';
        A =(-2)+3*4;

         IF(!(A > 0) && B<2||C==3){
               B=9;
               E[0]=E[0]+2.2;
         }ELSE{
               B=0;
         }

        IF(A == 9 && B == 1){
            C=1;
        }
        FOR(I=50:(-1):(-5)){
            C=A+1;
        }
        READ(A);
        WRITE("Hello World",A,B,C);
}