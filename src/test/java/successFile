VAR_GLOBAL {
   INTEGER A,B,C;
   CHAR Z[5],R[1],D[10];
    FLOAT E[5];
    INTEGER I;
}
DECLARATION {
    CONST INTEGER Y = 5;
    CONST INTEGER X = 4;
}

INSTRUCTION {
        E[0]=1.0;
        Z[0]='h';
        A =(-2)*3+4;
         IF(!(A > 0) && B<2||C==3){
               B=9;
         }ELSE{
               B=0;
         }

        A=3; B=1; C=3;
        IF(A == 9 && B == 1){
            A=1;
        }
        FOR(I=50.2:(-1):(-5)){
            A=A+1;
        }
}